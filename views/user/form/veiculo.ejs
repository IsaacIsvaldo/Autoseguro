<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
  <title>Post Create &mdash; Stisla</title>

  <!-- General CSS Files -->
  <link rel="stylesheet" href="/assets/modules/bootstrap/css/bootstrap.min.css">
  <link rel="stylesheet" href="/assets/modules/fontawesome/css/all.min.css">

  <!-- CSS Libraries -->
  <link rel="stylesheet" href="/assets/modules/summernote/summernote-bs4.css">
  <link rel="stylesheet" href="/assets/modules/jquery-selectric/selectric.css">
  <link rel="stylesheet" href="/assets/modules/bootstrap-tagsinput/dist/bootstrap-tagsinput.css">

  <!-- Template CSS -->
  <link rel="stylesheet" href="/assets/css/style.css">
  <link rel="stylesheet" href="/assets/css/components.css">

<body>
  <div id="app">
    <div class="main-wrapper main-wrapper-1">
      <div class="navbar-bg"></div>
      <!--Nav bar-->
      <%- include ../partials/navbar.ejs %>
        <!--sidebar-->

        <%- include ../partials/sidebar.ejs %>
          <div class="main-content">
            <section class="section">
              <div class="section-header">
                <div class="section-header-back">
                  <a href="#" class="btn btn-icon"><i class="fas fa-arrow-left"></i></a>
                </div>
                <div class="section-header-breadcrumb">

                  <div class="breadcrumb-item">Adicionar nova viatura</div>
                </div>
              </div>

              <div class="section-body">
                <div class="row">
                  <div class="col-12">
                    <div class="card">
                      <div class="card-header">
                       
                      </div>
                      <div class="card-body">
                        <form method="POST" action="/user/adicionar_veiculo/save" class="needs-validation" novalidate="" >
                          <div class="row">
                            <div class="form-group col-4">
                              <label for="frist_name">Tipo de Viatura:</label>
                              <select class="form-control selectric" name="tipo">
                                <option value="Carro">Carro</option>
                                <option value="Camiao">Camião</option>
                                <option value="Motorizada">Motorizada</option>
                              </select>
                            </div>
                            <div class="form-group col-4">
                              <label for="" class="d-block">Peso:</label>
                              <select id="slctprovincia" name="peso"  class="form-control ">
                                <option value="PBTC">PBTC</option>
                                <option value="PBT">PBT</option>
                              </select>
                                
                              </select>
                            </div>
                            <div class="form-group col-4">
                              <label for="frist_name">Lotação:</label>
                              <select class="form-control selectric" name="lotacao">
                                <option value="5">5 lugares</option>
                                <option value="7">7 Lugares</option>
                                <option value="2">2 lugares</option>
                                <option value="14">14 Lugares</option>
                              </select>
                            </div>
                            </div>
                            <div class="row">
                              <div class="form-group col-4">
                                <label for="frist_name">Combustivel:</label>
                                <select class="form-control selectric" name="combustivel">
                                  <option value="Gasolina">Gasolina</option>
                                  <option value="Gasoleo">Gasoleo</option>
                                  
                                </select>
                              </div>
                              <div class="form-group col-4">
                                <label for="frist_name">Tipo de Caixa:</label>
                                <select class="form-control selectric" name="tipo_caixa">
                                  <option value="Aberto">Aberto</option>
                                  <option value="Fechado">Fechado</option>
                                </select>
                              </div>
                              <div class="form-group col-4">
                                <label for="frist_name">Distancia entre Eixos:</label>
                                <select class="form-control selectric" name="distancia_eixo">
                                  <option value="200">200 Cm</option>
                                  <option value="250">250 Cm</option>
                                  <option value="300">300 Cm</option>
                                  <option value="450">450 Cm</option>
                                </select>
                              </div>

                              </div>
                              <div class="row">
                                <div class="form-group col-4">
                                  <label for="" class="d-block">Cilindrada:</label>
                                  <input id="" type="text" class="form-control" name="cilindrada"
                                    autofocus required    />
                                  <div class="invalid-feedback">
                                    matriculanao pode estar vazio
                                    <p id="title"></p>
                                  </div>
    
                                </div>
                                <div class="form-group col-4">
                                  <label for="frist_name">Nº: de Cilindro</label>
                                  <select class="form-control selectric" name="numero_cilindro">
                                    <option value="1">1</option>
                                    <option value="2">2</option>
                                    <option value="3">3</option>
                                    <option value="4">4</option>
                                  </select>
                                </div>
                                <div class="form-group col-4">
                                  <label for="" class="d-block">Medidas Pneumaticos:</label>
                                  <input id="" type="text" class="form-control" name="medida_pneumaticos"
                                    autofocus required    />
                                  <div class="invalid-feedback">
                                    matriculanao pode estar vazio
                                    <p id="title"></p>
                                  </div>
    
                                </div>
                              </div>
                            <div class="row">
                              <div class="form-group col-4">
                                <label  class="d-block">Nº do quadro</label>
                                <input  type="text" class="form-control "
                                  data-indicator="pwindicator" name="numero_quadro" autofocus required>
                                <div class="invalid-feedback">
                                  password nao pode estar vazio
  
                                </div>
                              </div>
                              <div class="form-group col-4">
                                <label for="frist_name">Tara</label>
                                <select class="form-control selectric" name="tara">
                                  <option value="750">750 kg</option>
                                  <option value="800">800Kg</option>
                                  <option value="900">900Kg</option>
                                  
                                </select>
                              </div>
                              <div class="form-group col-4">
                                <label  class="d-block">Nº do Motor</label>
                                <input  type="text" class="form-control "
                                  data-indicator="pwindicator" name="numero_motor" autofocus required>
                                <div class="invalid-feedback">
                                  password nao pode estar vazio
  
                                </div>
                              </div>
                              
                            </div>

                            <div class="row">
               
                              <div class="form-group col-4">
                                <select id="slctmarca" onchange="marcamodelo(this.id,'slctmodelo')" name="marca" class="form-control ">
                                  <option value="FERRARI">FERRARI</option>
                                  <option value="NISSAN">NISSAN</option>
                                  <option value="BMW">BMW</option>
                                  <option value="TOYOTA">TOYOTA</option>
                                  <option value="AGRALE">AGRALE</option>
                                  <option value="ASTON MARTIN">ASTON MARTI</option>
                                  <option value="BENTLEY">BENTLEY</option>
                                  <option value="BUGATTI">BUGATTI</option>
                                  <option value="CHANGAN">CHANGAN</option>
                                  <option value="GM/CHEVROLET">GM/CHEVROLET</option>
                                </select>
                                
                
                              </div>
                              <div class="form-group col-4">
                                <select name="modelo" id="slctmodelo" class="form-control "></select>
                              </div>
                              <div class="form-group col-4">
                               
                                <select class="form-control selectric" name="cor">
                                  <option value="PRETO">PRETO</option>
                                  <option value="AZUL">AZUL</option>
                                 
                                  
                                </select>
          
                              </div>
                            </div>

                            <div class="row">
                              
                              
                              
                              <div class="form-group col-6">
                                <label for="" class="d-block">Matricula:</label>
                                <input id="" type="text" class="form-control" maxlength="11" minlength="10" placeholder="AA-00-00-AA" name="matricula"
                                  autofocus required >
                                <div class="invalid-feedback">
                                  matriculanao pode estar vazio
                                  <p id="title"></p>
                                </div>
          
                              </div>
                              <div class="form-group col-6">
                                <label for="" class="d-block">Serviço:</label>
                                <select class="form-control selectric" name="servico">
                                  <option value="particular">particular</option>
                                  <option value="Empresa">Empresarial</option>
                                  
                                </select>
                              </div> 
                            </div>

                          <div class="form-divider">
                            <input  type="hidden" class="form-control" name="id" value="<%= user.id %> ">
                          </div>
         


                          <div class="form-group">
                            <div class="custom-control custom-checkbox">
                              <input type="checkbox" name="agree" class="custom-control-input" id="agree" autofocus
                                required>
                              <label class="custom-control-label" for="agree">Concordo com as condições</label>
                              
                            </div>
                          </div>

                          <div class="form-group">
                            <button type="submit" class="btn btn-primary btn-lg btn-block">
                              Cadastrar Viatura
                            </button>
                          </div>
                        </form>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </section>
          </div>
          <footer class="main-footer">
            <div class="footer-left">
              Copyright &copy;
              <script>
                document.write(new Date().getFullYear())
              </script>
            </div>
            <div class="footer-right">

            </div>
          </footer>
    </div>
  </div>

  <!-- General JS Scripts -->
  <script src="/assets/modules/jquery.min.js"></script>
  <script src="/assets/modules/popper.js"></script>
  <script src="/assets/modules/tooltip.js"></script>
  <script src="/assets/modules/bootstrap/js/bootstrap.min.js"></script>
  <script src="/assets/modules/nicescroll/jquery.nicescroll.min.js"></script>
  <script src="/assets/modules/moment.min.js"></script>
  <script src="/assets/js/stisla.js"></script>
  <script src="/script/MarcaModelo.js"></script>

  <!-- JS Libraies -->

  <script src="/assets/modules/summernote/summernote-bs4.js"></script>

  <script src="/assets/modules/bootstrap-tagsinput/dist/bootstrap-tagsinput.min.js"></script>

  <!-- Page Specific JS File -->
  <script src="/assets/js/page/features-post-create.js"></script>

  <!-- Template JS File -->
  <script src="/assets/js/scripts.js"></script>
  <script src="/assets/js/custom.js"></script>
</body>

</html>