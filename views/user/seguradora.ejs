<body>
  <div id="app">
    <div class="main-wrapper main-wrapper-1">
      <div class="navbar-bg"></div>
       <!--Nav bar-->
       <%- include partials/navbar.ejs %> 
       <!--sidebar-->
     
 <%- include partials/sidebar.ejs %> 

      <!-- Main Content -->
      <div class="main-content">
        <section class="section">
          <div class="section-header">
            <h1>Mapa dinâmico</h1>
            <div class="section-header-breadcrumb">
              <div class="breadcrumb-item active"><a href="#">Dashboard</a></div>
              <div class="breadcrumb-item"><a href="#"> Maps</a></div>
            
            </div>
          </div>

          <div class="section-body">
           
            <p class="section-lead">
             .
            </p>

            <div class="row">
              <div class="col-12">
                <div class="card">
                  <div class="card-header">
                    <h4></h4>
                  </div>
                  <div class="card-body">
                    <div id="map" class="map"></div>
                    
                    <div class="text-center mt-4">
                     <!--Aqui esta um botao-->
                    </div>
                    <div class="mt-4">
                      <ul id="instructions" class="list-unstyled list-unstyled-border"></ul>
                    </div>
                  </div>
                </div>
               
                  
              </div>
              
         
            </div>
            <div class="row">
              <div class="col-12 col-md-6 col-lg-6">
               
          
                  
                      <div class="pricing">
                        <div class="pricing-title">
                       Auto Seguro
                        </div>
                        <div class="pricing-padding">
                          <div class="pricing-price">
                          </div>
                          <div class="pricing-details">
                            <div class="pricing-item">
                              <div class="pricing-item-icon"><i class="fas fa-check"></i></div>
                              <div class="pricing-item-label"><%=seguradora.descricao%></div>
                            </div>
                          
                          </div>
                        </div>
                        <div class="pricing-cta">
                          
                        </div>
                      </div>
                    
           
               
               
              </div>
              <div class="col-12 col-md-6 col-lg-6">
             Informação da seguradora
                <div class="row">
                  <div class="form-group col-6">
                    
                    <input id="password" disabled class="form-control pwstrength" value="Contacto:<%= seguradora.whatsapp%>" >
                    </div>
                    <div class="form-group col-6">
                      <input id="password" disabled class="form-control pwstrength" value="Nome:<%=seguradora.name%>" >
                    
                       </div>
                  </div>
                  <div class="row">
                    <div class="form-group col-6">
                      
                      <input id="password" disabled class="form-control pwstrength" value="Lng:<%=seguradora.lng%>" >
                      </div>
                      <div class="form-group col-6">
                        <input id="password2" disabled class="form-control" value="Lat:<%=seguradora.lat%>">
                        
                        </div>
                  
              

                  </div>
      
                  
                </div>
     
          </div>
        </section>
      </div>
    <input type="hidden" id="lat" value="<%= seguradora.lat %> ">
    <input type="hidden" id="lng" value="<%= seguradora.lng %> ">
        <footer class="main-footer">
            <div class="footer-left">
              Copyright &copy;
              <script>
                document.write(new Date().getFullYear())
              </script>
            </div>
            <div class="footer-right">

            </div>
          </footer>
    </div>
  </div>
