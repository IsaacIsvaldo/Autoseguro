<body>
  <div id="app">
    <div class="main-wrapper main-wrapper-1">
      <div class="navbar-bg"></div>
      <%- include partials/navbar.ejs %> 
      <!--sidebar-->
    
<%- include partials/sidebar.ejs %> 

      <!-- Main Content -->
      <div class="main-content">
        <section class="section">
          <div class="section-header">
            <div class="section-header-breadcrumb">
              <div class="breadcrumb-item active"><a href="#">Dashboard</a></div>
              <div class="breadcrumb-item"><a href="#">Components</a></div>
            
            </div>
          </div>

          <div class="section-body">
     <% if(apolice != null) {%> 
            
              <div class="col-12">
                <div class="row">
                <% if(apolice.responsabilidade_civil == 1 ){%>
                  <div class="col-lg-4 mb-4">
                    <div class="card card-large-icons">
                      <div class="card-icon bg-primary text-white">
                        <i class="fas fa-user-shield"></i>
                      </div>
                      <div class="card-body">
                        <p>Responsabilidade civil.</p>
                        
                      </div>
                    </div>
                  </div>
                 <%}%>
                <% if(apolice.ocupantes == 1){%>
                  <div class="col-lg-4">
                    <div class="card card-large-icons">
                      <div class="card-icon bg-primary text-white">
                        <i class="fas fa-user"></i>
                      </div>
                      <div class="card-body">
                        <p>Ocupantes.</p>
                        
                      </div>
                    </div>
                  </div>
                 <%}%>
                <% if(apolice.furtos == 1){%>
                  <div class="col-lg-4">
                    <div class="card card-large-icons">
                      <div class="card-icon bg-primary text-white">
                        <i class="fas fa-car"></i>
                      </div>
                      <div class="card-body">
                        <p>Furtos ou Roubos.</p>
                        
                      </div>
                    </div>
                  </div>
                 <%}%>
                
                <% if(apolice.fenomenos_naturais== 1){%>
                  <div class="col-lg-4">
                    <div class="card card-large-icons">
                      <div class="card-icon bg-primary text-white">
                        <i class="fas fa-car"></i>
                      </div>
                      <div class="card-body">
                        <p>Fenomenos Naturais.</p>
                        
                      </div>
                    </div>
                  </div>
                 <%}%>
                <% if(apolice.quebras_isoladas_vidros == 1){%>
                  <div class="col-lg-4">
                    <div class="card card-large-icons">
                      <div class="card-icon bg-primary text-white">
                        <i class="fas fa-car"></i>
                      </div>
                      <div class="card-body">
                        <p>Quebras de Vidros.</p>
                        
                      </div>
                    </div>
                  </div>
                 <%}%>
                
                <% if(apolice.queda_antenas == 1){%>
                  <div class="col-lg-4">
                    <div class="card card-large-icons">
                      <div class="card-icon bg-primary text-white">
                        <i class="fas fa-car"></i>
                      </div>
                      <div class="card-body">
                        <p>Queda de antenas.</p>
                        
                      </div>
                    </div>
                  </div>
                 <%}%>
                
                <% if(apolice.queda_Paineis_solares == 1){%>
                  <div class="col-lg-4">
                    <div class="card card-large-icons">
                      <div class="card-icon bg-primary text-white">
                        <i class="fas fa-car"></i>
                      </div>
                      <div class="card-body">
                        <p>Queda de Paines Solares.</p>
                        
                      </div>
                    </div>
                  </div>
                 <%}%>
              
                <% if(apolice.vandalismo == 1){%>
                  <div class="col-lg-4">
                    <div class="card card-large-icons">
                      <div class="card-icon bg-primary text-white">
                        <i class="fas fa-car"></i>
                      </div>
                      <div class="card-body">
                        <p>Vandalismo Ocorrentes.</p>
                        
                      </div>
                    </div>
                  </div>
                 <%}%>
                
                <% if(apolice.honorarios_tecnicos == 1){%>
                  <div class="col-lg-4">
                    <div class="card card-large-icons">
                      <div class="card-icon bg-primary text-white">
                        <i class="fas fa-car"></i>
                      </div>
                      <div class="card-body">
                        <p>Honorarios Tecnicos.</p>
                        
                      </div>
                    </div>
                  </div>
                 <%}%>
                <% if(apolice.incendio  == 1){%>
