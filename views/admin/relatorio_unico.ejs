  <script src="/assets/modules/sweetalert/sweetalert2.all.min.js"></script>

  <script> 
    function confirmar(id){
    
        
    Swal.fire({
      title: 'Tens a certeza que deseja executar esta acção?',
    icon:'warning',
      showCancelButton: true,
      confirmButtonColor:'#3085d6',
      cancelButtonColor:'#d33',
      confirmButtonText: 'Confirmar',
    
    }).then((result) => {
      /* Read more about isConfirmed, isDenied below   */
      if (result.isConfirmed) {
    
       window.location = '/admin/sinitro/integral/'+id;
      } else if (result.isDenied) {
        Swal.fire('Erro ao eliminar', '', 'info')
      }
    })
      }
    
      function confirmar2(id){
    
        
    Swal.fire({
      title: 'Tens a certeza que deseja executar esta acção?',
    icon:'warning',
      showCancelButton: true,
      confirmButtonColor:'#3085d6',
      cancelButtonColor:'#d33',
      confirmButtonText: 'Confirmar',
    
    }).then((result) => {
      /* Read more about isConfirmed, isDenied below   */
      if (result.isConfirmed) {
    
       window.location = '/admin/sinitro/perda-total/'+id;
      } else if (result.isDenied) {
        Swal.fire('Erro ao eliminar', '', 'info')
      }
    })
      }
    </script>

